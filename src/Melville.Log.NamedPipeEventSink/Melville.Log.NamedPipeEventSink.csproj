<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    
    <PropertyGroup>
        <Version>0.1.0</Version>
        <Authors>John Melville, MS, MD</Authors>
        <Company>Melville Computing</Company>
        <Title>Melville.Log.NamedPipeEventSink</Title>
        <PackageDescription>A Serilog sink that outputs to my private log viewer.</PackageDescription>
        <RepositoryUrl>https://github.com/DrJohnMelville/Melville</RepositoryUrl>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <IsPackable>true</IsPackable>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Serilog" Version="2.9.0" />
      <PackageReference Include="Serilog.Formatting.Compact" Version="1.1.0" />
      <PackageReference Include="System.Security.Principal.Windows" Version="4.7.0" />
    </ItemGroup>

</Project>
