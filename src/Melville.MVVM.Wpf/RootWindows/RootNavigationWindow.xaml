<Window x:Class="Melville.MVVM.Wpf.RootWindows.RootNavigationWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:viewFrames="clr-namespace:Melville.MVVM.Wpf.ViewFrames"
        xmlns:waitingServices="clr-namespace:Melville.MVVM.Wpf.WaitingServices"
        mc:Ignorable="d"
        Title="RootNavigationWindow" Height="450" Width="800">
    <waitingServices:WaitMessageView>
        <viewFrames:ViewFrame HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Content="{Binding Content}"/>
    </waitingServices:WaitMessageView>
</Window>
