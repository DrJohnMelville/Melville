<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net5.0</TargetFramework>
        <Nullable>enable</Nullable>
    </PropertyGroup>
    <PropertyGroup>
        <Version>0.1.4</Version>
        <Authors>John Melville, MS, MD</Authors>
        <Company>Melville Computing</Company>
        <Title>Melville.IOC.Interception</Title>
        <PackageDescription>This is a binding that allows Melville.IOC Castle.Core Interceptors.</PackageDescription>
        <RepositoryUrl>https://github.com/DrJohnMelville/Melville</RepositoryUrl>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>
    <ItemGroup>
        <PackageReference Include="Castle.Core" Version="4.4.0" />
        <PackageReference Include="NuGet.Build.Tasks.Pack" Version="5.4.0" />
        <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="3.1.5" />
    </ItemGroup>
    <ItemGroup>
        <ProjectReference Include="..\Melville.IOC\Melville.IOC.csproj" />
    </ItemGroup>

</Project>
