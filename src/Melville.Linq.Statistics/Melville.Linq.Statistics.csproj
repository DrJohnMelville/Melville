
<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net7.0-windows</TargetFramework>
    <DebugType>Full</DebugType>
    <UseWPF>true</UseWPF>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <RootNamespace>LinqStatistics</RootNamespace>
    <Nullable>annotations</Nullable>
  </PropertyGroup>


  <PropertyGroup>
    
    
    
    <Title>Melville.Linq.Statistics</Title>
    
    <PackageDescription>A simple data analysis framework that works well with LinqPad.</PackageDescription>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>


  <ItemGroup>
    <PackageReference Include="LINQPad.Reference" Version="1.3.0" />
    <PackageReference Include="Accord.Statistics" Version="3.8.0" />
    <PackageReference Include="Castle.Core" Version="5.1.1" />
    <PackageReference Include="EPPlus.Core" Version="1.5.4" />
    <PackageReference Include="LINQPad.Runtime" Version="7.6.6" />
  </ItemGroup>


  <ItemGroup>
    <ProjectReference Include="..\Melville.CurveFit\Melville.CurveFit.csproj" />
  </ItemGroup>
  
</Project>